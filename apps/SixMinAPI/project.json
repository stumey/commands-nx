  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": [
        "dist/apps/SixMinAPI"
      ],
      "options": {
        "configuration": "Debug",
        "noDependencies": true
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "lint": {
      "executor": "@nx-dotnet/core:format"
